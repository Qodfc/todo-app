<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox alignment="CENTER" minHeight="400" minWidth="600" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lsb.controller.MainController">
   <children>
      <SplitPane dividerPositions="0.2">
        <items>
          <AnchorPane minHeight="0.0">
               <children>
                  <VBox>
                     <children>
                        <Button fx:id="btn" mnemonicParsing="false" text="Button" />
                        <Button mnemonicParsing="false" text="Button" />
                        <Button mnemonicParsing="false" text="Button" />
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
          <AnchorPane minHeight="400" minWidth="600">
               <children>
                  <WebView fx:id="webView" />
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </children>
</VBox>
